@model IEnumerable<WebApplication1.Controllers.DUser>

@{
    ViewData["Title"] = "CustomerPage";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<link rel="stylesheet" href="~/lib/AdminLTE/adminlte.min.css" />
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/ionic/1.3.2/css/ionic.min.css" />

<a asp-controller="Admin" asp-action="Index">← 戻る</a>
<br />
<br />
<div class="info-box">
    <span class="info-box-icon bg-green"><i class="ion ion-ios-people-outline"></i></span>

    <div class="info-box-content">
        <span class="info-box-text"><h1>ユーザー</h1></span>
        <a asp-area="Identity" asp-page="/Account/Register">顧客登録</a>

    </div>
</div>

<table class="table table-striped table-bordered" style="width: 100%" id="customerTable">
    <thead>
        <tr>
            <th>ユーザーメール</th>
            <th>ユーザー役割</th>
        </tr>
    </thead>
    <tbody>

        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Username</td>
                <td>@user.Role</td>
            </tr>
        }

    </tbody>
</table>